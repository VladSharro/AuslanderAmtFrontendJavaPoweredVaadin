# Nginx configuration for ASPD FE

server {
        listen 80;
        listen [::]:80;
        
        root /var/www/aspd/asdfrontend/browser;

        # Add index.php to the list if you are using PHP
        index index.html asdfrontend/browser/index.html;

        server_name aspd;

        location /var/www/aspd/asdfrontend {
                # First attempt to serve request as file, then
                # as directory, then fall back to displaying a 404.
                autoindex on;
                try_files $uri $uri/ =404;
        }
}